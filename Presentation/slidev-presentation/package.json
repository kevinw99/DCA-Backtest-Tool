{
  "name": "dca-backtest-presentation",
  "version": "1.0.0",
  "description": "Grid-Based DCA Trading Simulator Presentation - Adaptive Trading Strategies",
  "type": "module",
  "scripts": {
    "dev": "slidev",
    "build": "slidev build --base /presentation/ && cp public/*.html dist/presentation/",
    "export": "slidev export",
    "start": "npx serve dist",
    "dev:methodology": "slidev methodology-slides.md",
    "build:methodology": "slidev build methodology-slides.md --base /methodology/ && cp public/*.html dist/methodology/",
    "export:methodology": "slidev export methodology-slides.md"
  },
  "keywords": [
    "slidev",
    "presentation",
    "dca",
    "trading"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@slidev/cli": "^52.9.1",
    "@slidev/theme-default": "^0.25.0"
  },
  "dependencies": {
    "serve": "^14.2.5"
  }
}
