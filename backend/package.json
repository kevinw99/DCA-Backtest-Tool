{
  "name": "stock-trading-backend",
  "version": "1.0.0",
  "description": "Backend API for Stock Trading Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "update-splits": "node scripts/updateExistingData.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "quality:duplication": "jscpd .",
    "quality:complexity": "cr services/**/*.js",
    "validate": "npm run lint && npm run format:check && npm run test:coverage",
    "prepare": "cd .. && husky backend/.husky || true"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "complexity-report": "^2.0.0-alpha",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jscpd": "^4.0.5",
    "lint-staged": "^16.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2"
  },
  "keywords": [
    "stock",
    "trading",
    "dashboard",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "services/**/*.js",
      "!services/**/*.test.js"
    ]
  }
}
